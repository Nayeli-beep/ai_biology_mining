from Bio import Entrez
import pandas as pd

Entrez.email="nruiz@ifc.unam.mx"
handle=Entrez.esearch(db="pubmed", term="artificial intelligence",retmax=20)
record = Entrez.read (handle)
record["IdList"]
idlist=record["IdList"]

handle=Entrez.efetch(db="pubmed", id=idlist, rettype="abstract", retmode="text") 
print(handle.read())
df= pd.DataFrame({"PMID","Biology Field","Subset", "Title","Abstract", "Keywords", "Time Period", "URL"})
